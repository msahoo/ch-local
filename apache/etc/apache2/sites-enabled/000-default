<VirtualHost *:80>
		Options All FollowSymLinks Indexes
        AccessFileName  .ch.htaccess .jument.htaccess
        
        ServerAdmin      webmaster@localhost
        DocumentRoot     /var/www/ch9/web
        ServerName       www.collegehumor.local
		
		<Directory /var/www/ch9/web>
			Options FollowSymLinks
			AllowOverride All
		</Directory>

		PassEnv          USER
		SetEnv ENV_TYPE  dev
		LogLevel         debug
        ErrorLog         /var/log/apache2/collegehumor.local-error_log
        
</VirtualHost>